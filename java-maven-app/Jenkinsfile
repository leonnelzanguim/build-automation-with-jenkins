def gv
pipeline {
  agent any
  stages {
    stage("init") {
      steps {
        script {
          gv = load "java-maven-app/script.groovy"
        }
      }
    }
     stage("build Jar") {
      steps {
        script {
          gv.buildJar()
        }
      }
    }
    stage("deploy") {
      steps { 
        script {
          gv.deployApp()   
        }
      }
    }
  }
}
